<?xml version="1.0" encoding="UTF-8"?>
<dataSplitter xmlns="data-splitter:3" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="data-splitter:3 file://data-splitter-v3.0.xsd" version="3.0">

  <!-- Match each record -->
  <split delimiter="\n">
    <group value="$1">
      <regex pattern="^([^ ]+) ([^ ]+) ([^ ]+) \[([^\]]+)\] &quot;([^&quot;]+)&quot; ([^&quot;]+) &quot;([^&quot;]+)&quot; &quot;(.*[^\\])&quot;(.*)$">
        <data value="$1" />
        <data value="$2" />
        <data value="$3" />
        <data value="$4" />
        <group value="$5">
          <regex pattern="^.*">
            <data>
              <group>
                <split delimiter=" ">
                  <data value="$1" />
                </split>
              </group>
            </data>
          </regex>
        </group>
        <group value="$6">
          <regex pattern="^.*">
            <group>
              <split delimiter=" ">
                <data value="$1" />
              </split>
            </group>
          </regex>
        </group>
        <data value="$9" />
        <data>
          <data name="Referer" value="$7" />
          <data name="User-Agent" value="$8" />
        </data>
      </regex>
    </group>
  </split>
</dataSplitter>